@{
    ViewData["Title"] = "Python Script Result";
}

<div class="container mt-4">
    <h1>Python Script Result</h1>
    
    <div class="card mt-4">
        <div class="card-header bg-primary text-white">
            <h5 class="mb-0">Output</h5>
        </div>
        <div class="card-body">
            <pre class="bg-light p-3">@ViewBag.Result</pre>
        </div>
        <div class="card-footer">
            <a asp-action="Index" class="btn btn-outline-primary">Back to Python Integration</a>
        </div>
    </div>
</div>
